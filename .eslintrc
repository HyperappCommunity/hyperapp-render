{
  "extends": ["airbnb", "eslint-config-prettier", "eslint-config-prettier/react"],
  "rules": {
    "import/prefer-default-export": "off",
    "no-continue": "off",
    "no-plusplus": "off",
    "no-restricted-syntax": "off",
    "prefer-template": "off"
  },
  "settings": {
    "react": {
      "pragma": "h"
    }
  }
}
